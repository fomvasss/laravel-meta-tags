{
    "name": "fomvasss/laravel-meta-tags",
    "description": "A package to manage SEO (meta-tags, xml-fields, etc.)",
    "type": "composer-package",
    "keywords": ["meta tags", "meta", "tags", "seo", "seo", "sitemap", "xml-map", "laravel"],
    "authors": [
        {
            "name": "Vasyl Fomin",
            "email": "fomvasss@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php" : ">=7.0.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/database": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/view": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\LaravelMetaTags\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\LaravelMetaTags\\ServiceProvider"
            ],
            "aliases": {
                "MetaTag": "Fomvasss\\LaravelMetaTags\\Facade"
            }
        }
    }
}
